<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet>
        <servlet-name>SvlAutentificacion</servlet-name>
        <servlet-class>Controlador.SvlAutentificacion</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>cgh</servlet-name>
        <servlet-class>Controlador.cgh</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SvlCliente</servlet-name>
        <servlet-class>Controlador.SvlCliente</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SvlEmpleado</servlet-name>
        <servlet-class>Controlador.SvlEmpleado</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SvlInsumos</servlet-name>
        <servlet-class>Controlador.SvlInsumos</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>nb</servlet-name>
        <servlet-class>Controlador.nb</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SvlPedido</servlet-name>
        <servlet-class>Controlador.SvlPedido</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SvlOrden</servlet-name>
        <servlet-class>Controlador.SvlOrden</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SvlAutentificacion</servlet-name>
        <url-pattern>/SvlAutentificacion</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SvlCliente</servlet-name>
        <url-pattern>/SvlCliente</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SvlEmpleado</servlet-name>
        <url-pattern>/SvlEmpleado</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SvlInsumos</servlet-name>
        <url-pattern>/SvlInsumos</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>nb</servlet-name>
        <url-pattern>/nb</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SvlPedido</servlet-name>
        <url-pattern>/SvlPedido</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SvlOrden</servlet-name>
        <url-pattern>/SvlOrden</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
